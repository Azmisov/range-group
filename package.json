  "license": "LGPL-3.0",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "benchmark": "^2.1.4",
    "clean-jsdoc-theme": "github:Azmisov/clean-jsdoc-theme",
    "core-js": "^3.25.5",
    "jest": "^29.5.0",
    "rollup": "^3.5.0",
    "seed-random": "^2.2.0"

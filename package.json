{
  "name": "range-group",
  "version": "1.0.0",
  "description": "Data structure to hold a collection of ranges. It has builtin support for numbers, dates, and strings, fast O(log(log(N))) bounds, and provides comprehensive set operations.",
  "keywords": [
    "data structure",
    "collection",
    "range",
    "interval",
    "set",
    "interpolation search",
    "binary search"
  ],
